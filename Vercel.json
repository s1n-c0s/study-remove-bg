{
    "version": 2,
    "builds": [
        {
            "src": "api/remove_bg.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/remove_bg",
            "dest": "api/remove_bg.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}