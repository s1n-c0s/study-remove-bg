{
    "version": 2,
    "builds": [
        {
            "src": "api/remove_bg.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "functions": {
        "api/index.js": {
            "memory": 1024,
            "maxDuration": 10
        },
        "routes": [
            {
                "src": "/api/remove_bg",
                "dest": "api/remove_bg.py"
            },
            {
                "src": "/(.*)",
                "dest": "/public/$1"
            }
        ]
    }